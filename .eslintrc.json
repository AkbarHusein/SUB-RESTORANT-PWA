{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "linebreak-style": 0,
    "no-underscore-dangle": 0,
    "operator-linebreak": 0,
    "no-restricted-globals": 0,
    "no-alert": 0,
    "indent": 0,
    "comma-dangle": 0,
    "no-console": 0,
    "quotes": 0,
    "implicit-arrow-linebreak": 0,
    "no-undef": 0
  }
}
